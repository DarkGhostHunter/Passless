{
  "name": "darkghosthunter/passless",
  "description": "Passless is a password-less authentication guard driver for Laravel 5.7",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Italo Baeza Cabrera",
      "email": "darkghosthunter@gmail.com"
    }
  ],
  "require": {
    "php" : "^7.1.3",
    "illuminate/auth": "5.8.*",
    "illuminate/config": "5.8.*",
    "illuminate/http": "5.8.*",
    "illuminate/cookie": "5.8.*",
    "illuminate/session": "5.8.*",
    "illuminate/events": "5.8.*",
    "illuminate/notifications": "5.8.*",
    "illuminate/support": "5.8.*"
  },
  "require-dev": {
    "orchestra/testbench": "3.8.*"
  },
  "autoload": {
    "psr-4": {
      "DarkGhostHunter\\Passless\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\" : "tests"
    }
  },
  "extra": {
    "laravel" : {
      "providers": [
        "DarkGhostHunter\\Passless\\PasslessServiceProvider"
      ]
    }
  }
}