{
  "name": "darkghosthunter/passless",
  "description": "Passless is a password-less authentication guard driver for Laravel 5.8",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Italo Baeza Cabrera",
      "email": "darkghosthunter@gmail.com"
    }
  ],
  "require": {
    "php" : "^7.2",
    "illuminate/auth": "^6.0||^7.0",
    "illuminate/config": "^6.0||^7.0",
    "illuminate/http": "^6.0||^7.0",
    "illuminate/cookie": "^6.0||^7.0",
    "illuminate/session": "^6.0||^7.0",
    "illuminate/events": "^6.0||^7.0",
    "illuminate/notifications": "^6.0||^7.0",
    "illuminate/support": "^6.0||^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.1||^5.0"
  },
  "autoload": {
    "psr-4": {
      "DarkGhostHunter\\Passless\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\" : "tests"
    }
  },
  "extra": {
    "laravel" : {
      "providers": [
        "DarkGhostHunter\\Passless\\PasslessServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

  },
  "config": {
    "sort-packages": true
  }
}
